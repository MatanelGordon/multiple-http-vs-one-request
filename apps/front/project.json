{
  "root": "apps/front",
  "sourceRoot": "apps/front/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "frameworkConfigFile": "@nxext/svelte/plugins/vite",
        "outputPath": "dist/apps/front",
        "assets": [
          {
            "glob": "/*",
            "input": "./public/**",
            "output": "./"
          }
        ],
        "tsConfig": "apps/front/tsconfig.app.json"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "frameworkConfigFile": "@nxext/svelte/plugins/vite",
        "outputPath": "dist/apps/front",
        "assets": [
          {
            "glob": "/*",
            "input": "./public/**",
            "output": "./"
          }
        ],
        "tsConfig": "apps/front/tsconfig.app.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/front/**/*.{ts,svelte,spec.ts}"
        ]
      }
    },
    "check": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "apps/front"
      }
    },
    "test": {
      "executor": "@nxext/vitest:vitest",
      "options": {
        "command": "run"
      }
    }
  }
}
